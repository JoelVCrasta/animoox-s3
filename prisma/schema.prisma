
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Icon {
  id         String    @id @default(auto()) @map("_id") @db.ObjectId
  iconStyle  String
  license    String
  category   String
  tags       String[]
  files      String[]
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@map("icons")
}


